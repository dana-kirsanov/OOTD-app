target 'OOTD-app' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OOTD-app
  
  pod 'Firebase/Core'

  pod ‘Firebase/Auth’

  pod ‘Firebase/Database’

  pod ‘Firebase/Storage’

  target 'OOTD-appTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'OOTD-appUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
